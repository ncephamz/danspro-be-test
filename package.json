{
  "name": "danspro-be-test",
  "version": "0.0.0",
  "main": "index.js",
  "engines": {
    "node": "12.16.3"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:generate": "npx sequelize-cli migration:generate --name",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:name": "npx sequelize-cli db:seed --seed",
    "seed:generate": "npx sequelize-cli seed:generate --name",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "reset:db": "npm run migrate:undo:all && npm run migrate && npm run seed"
  },
  "dependencies": {
    "confidence": "^4.0.2",
    "crypto": "^1.0.1",
    "dotenv": "^6.1.0",
    "events": "1.1.1",
    "excel4node": "^1.7.2",
    "joi": "^14.0.1",
    "jsftp": "2.0.0",
    "jsonwebtoken": "^8.3.0",
    "mariadb": "^3.0.0",
    "passport": "0.4.0",
    "passport-http": "0.3.0",
    "restify": "^8.3.3",
    "restify-cors-middleware": "^1.1.1",
    "restify-errors": "^8.0.0",
    "rewire": "^5.0.0",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1",
    "serve-static-restify": "^3.0.0",
    "util": "0.10.3",
    "uuid": "3.1.0",
    "validate.js": "0.12.0",
    "winston": "^2.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^2.0.15"
  }
}
